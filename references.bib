  shorttitle = {Nowcasting Epidemics of Novel Pathogens},
  author = {Wu, Joseph T. and Leung, Kathy and Lam, Tommy T. Y. and Ni, Michael Y. and Wong, Carlos K. H. and Peiris, J. S. Malik and Leung, Gabriel M.},
