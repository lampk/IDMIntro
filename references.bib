  keywords = {Communicable Diseases; Emerging,COVID-19,Disease Outbreaks,Epidemics,Epidemiology,Forecasting,History; 21st Century,Humans,Pandemics,Policy and public health in microbiology,Reproducibility of Results,Viral infection},
